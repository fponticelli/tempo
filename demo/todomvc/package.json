  "dependencies": {
    "dom": "0.0.3",
    "tempo-core": "^1.9.0",
    "tempo-dom": "^0.29.0",
    "tempo-std": "^0.7.0",
    "todomvc-app-css": "2.2.0",
    "todomvc-common": "1.0.5"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.3",
    "prettier": "^2.0.5",
    "cssnano": "^4.1.10",
    "babel-types": "^6.26.0"
  },
  "scripts": {
    "compile": "NODE_ENV=production && npx tsc --noEmit && parcel build src/index.html --public-url . --out-dir ./dist --no-minify",
