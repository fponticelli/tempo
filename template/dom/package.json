{
  "name": "tempo-template-dom",
  "version": "1.0.0",
  "description": "Generic getting started template for Tempo DOM.",
  "keywords": [
    "tempo",
    "ui",
    "framework",
    "typescript",
    "dom",
    "angular-alternative",
    "react-alternative"
  ],
  "main": "index.js",
  "dependencies": {
    "tempo-core": "^1.9.0",
    "tempo-dom": "^0.29.0",
    "tempo-std": "^0.7.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/jsdom": "^16.2.3",
    "babel-types": "^6.26.0",
    "cssnano": "^4.1.10",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "stealthy-require": "^1.1.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "canvas": "^2.6.1"
  },
  "scripts": {
    "compile": "NODE_ENV=production && npx tsc --noEmit && parcel build src/index.html --public-url . --out-dir ./dist",
    "clean": "rm -rf dist/* && rm -rf ./build/*",
    "build": "npm run clean && npm run compile",
    "start": "parcel src/index.html",
    "prettier": "prettier -c src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts"
  },
  "homepage": "https://github.com/fponticelli/tempo",
  "bugs": {
    "url": "https://github.com/fponticelli/tempo/issues"
  },
  "author": {
    "name": "Franco Ponticelli",
    "email": "franco.ponticelli@gmail.com",
    "url": "https://github.com/fponticelli"
  },
  "license": "Apache-2.0"
}
